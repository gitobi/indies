<table>
  <tr>
    <th>id</th>
    <th>type</th>
    <th>name</th>
    <th>picture</th>
    <th>created_at</th>
    <th>updated_at</th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td> <%= user.id %>  </td>
    <td> <%= user.type %>  </td>
    <td> <%= user.name %></td>
    <td> <%= user.picture %></td>
    <td> <%= user.created_at %></td>
    <td> <%= user.updated_at %></td>
    <td> <%= link_to 'Show', user %></td>
  </tr>
<% end %>
</table>

<br />

