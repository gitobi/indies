<h2> user info </h2>
<p>
<ul>
  <li> <%= @user.id %> </li>
  <li> <%= @user.type %> </li>
  <li> <%= @user.name %> </li>
  <li> <%= @user.picture %> </li>
  <li> <%= @user.created_at %> </li>
  <li> <%= @user.updated_at %> </li>
</ul>
</p>

<h3> Projects </h3>
<p>
<table>
  <tr>
    <th>id</th>
    <th>name</th>
    <th>picture</th>
    <th>created_at</th>
    <th>updated_at</th>
    <th></th>
  </tr>

<% @user.projects.each do |member| %>
  <tr>
    <td> <%= member.id %>  </td>
    <td> <%= member.type %> </td>
    <td> <%= member.name %> </td>
    <td> <%= member.picture %> </td>
    <td> <%= member.created_at %> </td>
    <td> <%= member.updated_at %> </li>
  </tr>
<% end %>
</table>
</p>


<h3> Teams </h3>
<p>
<table>
  <tr>
    <th>id</th>
    <th>name</th>
    <th>picture</th>
    <th>created_at</th>
    <th>updated_at</th>
    <th></th>
  </tr>

<% @user.teams.each do |member| %>
  <tr>
    <td> <%= member.id %>  </td>
    <td> <%= member.type %> </td>
    <td> <%= member.name %> </td>
    <td> <%= member.picture %> </td>
    <td> <%= member.created_at %> </td>
    <td> <%= member.updated_at %> </li>
  </tr>
<% end %>
</table>
</p>


